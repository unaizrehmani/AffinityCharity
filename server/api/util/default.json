                                  "containerPadding": "10px",
                                  "_meta": {
                                      "htmlID": "u_content_text_18",
                                      "htmlClassNames": "u_content_text"
                                  },
                                  "selectable": true,
                                  "draggable": true,
                                  "deletable": true,
                                  "color": "#000000",
